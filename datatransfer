                <a href="#overview" class="text-sm font-bold procore-text-orange hover:text-orange-700 transition duration-300">

                    &uarr; Back to Top

                </a>

            </div>

        </section>



        <!-- Section: Key Actions (Now at the bottom) - REMOVED -->



    </main>



    <!-- Footer -->

    <footer class="procore-bg text-white py-6 mt-10">

        <div class="max-w-7xl mx-auto px-4 sm:px-6 lg:px-8 text-center text-sm opacity-80">
            <!-- Footer content removed per user request -->
        </div>

    </footer>



    <!-- JavaScript for resolution toggle and smooth scrolling -->

    <script>

        console.log("Interactive site loaded. Custom JavaScript smooth scrolling enabled.");

        

        /**

         * Custom handler to force smooth scrolling for hash links and prevent page reload/redirect.

         */

        document.addEventListener('DOMContentLoaded', () => {

            document.querySelectorAll('a[href^="#"]').forEach(anchor => {

                anchor.addEventListener('click', function (e) {

                    // 1. Crucially prevents the default browser action (which can cause a page reload in some environments)

                    e.preventDefault();



                    const targetId = this.getAttribute('href');

                    const targetElement = document.querySelector(targetId);

                    

                    if (targetElement) {

                        // 2. Manually trigger the scroll behavior

                        targetElement.scrollIntoView({

                            behavior: 'smooth'

                        });

                        

                        // 3. Update the URL hash without triggering navigation, for bookmarking/history

                        if (history.pushState) {

                            history.pushState(null, null, targetId);

                        } else {

                            window.location.hash = targetId;

                        }

                    }

                });

            });

            

            // Initialize the dynamic roadmap view once DOM is ready

            renderRoadmap();

        });



        

        /**

         * Function to toggle resolution notes visibility for challenge cards.

         * @param {number} id - The ID of the challenge (e.g., 1 for challenge-1, 100 for Day 1).

         */

        function toggleResolution(id) {

            const resolutionDiv = document.getElementById('resolution-' + id);

            const button = document.getElementById('button-' + id);



            if (!resolutionDiv || !button) {

                console.error(`Elements for ID ${id} not found.`);

                return;

            }



            if (resolutionDiv.classList.contains('hidden')) {

                // Show notes -> Button should say "Hide..."
                resolutionDiv.classList.remove('hidden');
                
                // Set the correct "Hide" text
                if (id === 100) {
                    button.textContent = 'Hide Day 1 Follow Ups';
                } else if (id === 200) {
                    button.textContent = 'Hide Day 2 Follow Ups';
                } else if (id === 300) {
                    button.textContent = 'Hide Day 3 Follow Ups';
                } else if (id === 400) {
                    button.textContent = 'Hide Day 4 Follow Ups';
                } else if (id === 500) { 
                    button.textContent = 'Hide Day 5 Follow Ups';
                } else if (id === 607) { // <-- ADDED FOR DAY 6-7
                    button.textContent = 'Hide Day 6-7 Follow Ups';
                } else {
                    button.textContent = 'Hide Resolution/Examples'; // For Challenge 1-8
                }
                
                button.classList.add('bg-gray-400');

                button.classList.remove('procore-button');


            } else {

                // Hide notes -> Button should revert to "Show..."
                resolutionDiv.classList.add('hidden');
                
                // Set the correct "Show" text
                if(id === 400) {
                    button.textContent = 'Show Day 4 Follow Ups';
                } else if (id === 200) {
                    button.textContent = 'Show Day 2 Follow Ups';
                } else if (id === 300) {
                    button.textContent = 'Show Day 3 Follow Ups';
                } else if (id === 100) {
                    button.textContent = 'Show Day 1 Follow Ups';
                } else if (id === 500) { 
                    button.textContent = 'Show Day 5 Follow Ups';
                } else if (id === 607) { // <-- ADDED FOR DAY 6-7
                    button.textContent = 'Show Day 6-7 Follow Ups';
                } else {
                    button.textContent = 'Show Resolution/Examples';
                }
                
                button.classList.remove('bg-gray-400');

                button.classList.add('procore-button');

            }

        }

        

        /**

         * Function to toggle the visibility and icon of the Draft Roadmap section.

         */

        function toggleRoadmap() {

            const content = document.getElementById('draft-roadmap-content');

            const icon = document.getElementById('roadmap-toggle-icon');

            

            if (content.classList.contains('hidden')) {

                content.classList.remove('hidden');

                icon.classList.add('rotate-180');

            } else {

                content.classList.add('hidden');

                icon.classList.remove('rotate-180');

            }

        }

        

        /**

         * Function to toggle inline details visibility for a roadmap card.

         */

        function toggleDetails(element) {

            const detailDiv = element.querySelector('.roadmap-detail');

            if (detailDiv) {

                detailDiv.classList.toggle('hidden');

            }

        }





        // --- NEW SWIMLANE ROADMAP LOGIC ---

        

        // Data extracted and condensed from the PDF, now including the Reporter field as 'requester'

        const roadmapData = [

            { id: 'LRC-2226', team: 'Legal Operations', quarter: 'Q4-FY25', status: 'In Progress', summary: 'GTM Workforce Metrics', fullSummary: 'SFDC/Ironclad - GTM Legal Workforce Allocation Metrics (2026 Planning)', requester: 'Mick Cox' },

            { id: 'LRC-2176', team: 'Go-To-Market', quarter: 'Q4-FY25', status: 'In Progress', summary: 'Legal Opps Management', fullSummary: 'SFDC/Ironclad - Legal Opportunities Management', requester: 'Emily See' },

            { id: 'LRC-2188', team: 'Go-To-Market', quarter: 'Q4-FY25', status: 'In Progress', summary: 'Review Ironclad Workflows', fullSummary: 'SFDC/Ironclad - Legal Review Ironclad Workflows (Q4 Updates)', requester: 'Daniel Baker' },

            // MOVED TO BACKLOG

            { id: 'LRC-2213', team: 'Go-To-Market', quarter: 'Q2-FY26', status: 'Waiting for Customer', summary: 'Govt. Customer Contracts', fullSummary: 'SFDC/Ironclad /- Procore for Government- Customer Contracts', requester: 'Christyne Brennan' },

            { id: 'LRC-2175', team: 'Go-To-Market', quarter: 'Q4-FY25', status: 'In Progress', summary: 'Review Case Management', fullSummary: 'SFDC/Ironclad - Legal Review Case Management', requester: 'Emily See' },

            { id: 'LRC-1752', team: 'Go-To-Market', quarter: 'Q4-FY25', status: 'In Progress', summary: 'GTM Success Metrics', fullSummary: 'SFDC/Ironclad-GTM Legal Success Metrics & Analytics', requester: 'Emily See' },

            { id: 'LAC-2192', team: 'Go-To-Market', quarter: 'Q4-FY25', status: 'In Progress', summary: 'GTM Workforce Guide', fullSummary: 'SFDC/Ironclad GTML Workforce Allocation Dashboard Read. Me Guide Build', requester: 'Daniel Baker' },

            

            // MOVED TO BACKLOG

            { id: 'LRC-1320', team: 'Product, Privacy, & IP', quarter: 'Q2-FY26', status: 'In Progress', summary: 'Contract End Date System', fullSummary: 'SFDC/Ironclad We need a definitive system for the end of term date for customer contracts', requester: 'Matt Altaras' },

            

            { id: 'LAC-1777', team: 'Legal Operations', quarter: 'Q4-FY25', status: 'On Hold', summary: 'Ironclad SSA Dashboard', fullSummary: 'SFDC/Ironclad Commercial Order Form and Subscription Agreement Dashboard', requester: 'Daniel Baker' },

            

            // MOVED TO BACKLOG

            { id: 'LRC-2000', team: 'Legal Operations', quarter: 'Q2-FY26', status: 'In Progress', summary: 'Fix Blank Sales Segment', fullSummary: 'SFDC/Ironclad - Sales Segment is Blank in Ironclad for 1265 SSA records', requester: 'Daniel Baker' },

            

            // Excluded Systems - kept in data but won't be rendered due to filter logic below

            { id: 'LRC-2198', team: 'Product, Privacy, & IP', quarter: 'Q4-FY25', status: 'In Progress', summary: 'MTL Transaction DB Req.', fullSummary: 'MTL-Transaction Database Requirements for Regulatory Examinations', requester: 'Sunni Wallace' }, 

            

            { id: 'LRC-2222', team: 'Legal Operations', quarter: 'Q4-FY25', status: 'Open', summary: 'Vendor Contracts Reporting', fullSummary: 'SFDC/Ironclad - Reporting on Vendor Contracts in Tableau', requester: 'Sunni Wallace' },

            

            // Excluded Systems - kept in data but won't be rendered due to filter logic below

            { id: 'LRC-2199', team: 'Product, Privacy, & IP', quarter: 'Q4-FY25', status: 'In Progress', summary: 'MTL-ADTL Reporting', fullSummary: 'MTL-ADTL Reporting', requester: 'Srija Mokarala' }, 

            

            { id: 'LAC-1373', team: 'Finance', quarter: 'Q4-FY25', status: 'In Progress', summary: 'Deal Desk Dashboard', fullSummary: 'SFDC/Ironclad Deal Desk Dashboard', requester: 'Daniel Baker' },

            

            // MOVED TO BACKLOG

            { id: 'LRC-1991', team: 'Go-To-Market', quarter: 'Q2-FY26', status: 'In Progress', summary: 'Capture Security Exhibit', fullSummary: 'SFDC/Ironclad - Process for capturing when Security Exhibit is Attached to SSA', requester: 'Emily See' },

            { id: 'LRC-1993', team: 'Go-To-Market', quarter: 'Q2-FY26', status: 'Open', summary: 'PubSec Deals Reporting', fullSummary: 'SFDC/Ironclad - Opportunities Dashboard: New Tab-Ability to report on PubSec Deals- Direct v Indirect', requester: 'Christyne Brennan' },

            { id: 'LRC-1257', team: 'Product, Privacy, & IP', quarter: 'Q2-FY26', status: 'Open', summary: 'Design Beta Dashboard', fullSummary: 'SFDC/Ironclad - Design Beta Program Dashboard (IP Team)', requester: 'Jordan Ekelin' },

            { id: 'LRC-2233', team: 'Go-To-Market', quarter: 'Q2-FY26', status: 'On Hold', summary: 'Import SSAs to Ironclad', fullSummary: 'SFDC/Ironclad - Significant tranche of active SSAs to be imported to Ironclad (incl. data remediation)', requester: 'Alice Bisgrove' },

            

            // Excluded Systems - kept in data but won't be rendered due to filter logic below

            { id: 'LRC-2121', team: 'Office of the CLO', quarter: 'Q2-FY26', status: 'Open', summary: 'CLO Finance Dashboard', fullSummary: 'Pigment-CLO Finance Dashboard', requester: 'Daniel Baker' }, 

        ];



        // Custom mapping to treat Q2-FY26 as 'Backlog'

        const QUARTER_MAPPING = {

            'Q2-FY26': 'Backlog'

        };



        // --- Core Logic for System Grouping ---

        const getSystem = (fullSummary) => {

            if (fullSummary.startsWith('SFDC/Ironclad') || fullSummary.startsWith('SFDCIronclad')) {

                return 'SFDC/Ironclad';

            }

            if (fullSummary.startsWith('MTL')) {

                return 'MTL';

            }

            if (fullSummary.startsWith('Pigment')) {

                return 'Pigment';

            }

            return 'Other';

        };



        // 1. Process data: Add displayQuarter (Backlog) and System

        const processedData = roadmapData.map(item => ({

            ...item,

            displayQuarter: QUARTER_MAPPING[item.quarter] || item.quarter,

            system: getSystem(item.fullSummary)

        }));



        // 2. Determine unique quarters and systems

        // Only render SFDC/Ironclad projects (and any projects mapped to 'Other', though currently empty)

        const allSystems = [...new Set(processedData.map(item => item.system))]

            .filter(system => system === 'SFDC/Ironclad' || system === 'Other') 

            .sort();

            

        // Sort quarters manually to ensure Backlog is at the end

        const allQuarters = [...new Set(processedData.map(item => item.displayQuarter))].sort((a, b) => {

            // Force Q4-FY25 before Backlog

            if (a === 'Backlog') return 1;

            if (b === 'Backlog') return -1;

            return a.localeCompare(b);

        });

        

        // Retained for future filter use if needed

        let currentTeamFilter = 'All'; 

        let currentStatusFilter = 'All'; 



        /**

         * Converts status string to a CSS-friendly class name.

         */

        const getStatusClass = (status) => {

            return `status-${status.replace(/\s/g, '-')}`;

        };



        /**

         * Renders the main roadmap container using System Swimlanes.

         */

        function renderRoadmap() {

            const mainContainer = document.getElementById('main-roadmap-container');

            if (!mainContainer) return;



            mainContainer.innerHTML = '';

            

            // Iterate over Systems to create the main swimlanes

            allSystems.forEach(system => {

                // Filter all projects for this system, subject to filters

                const itemsInSystem = processedData.filter(item => 

                    item.system === system &&

                    (currentTeamFilter === 'All' || item.team === currentTeamFilter) &&

                    (currentStatusFilter === 'All' || item.status === currentStatusFilter)

                );



                if (itemsInSystem.length === 0) return; // Skip empty lanes



                // Create the System Lane container

                const systemLane = document.createElement('div');

                systemLane.className = 'system-lane-container';

                

                // System Header

                systemLane.innerHTML = `<h2 class="system-header">${system}</h2>`;



                // Create the Quarter flow container (horizontal scroll)

                const flowContainer = document.createElement('div');

                flowContainer.className = 'roadmap-container-flow';



                // 1. Iterate over Quarters to create columns inside this system lane

                allQuarters.forEach(quarter => {

                    const quarterColumn = document.createElement('div');

                    quarterColumn.className = 'quarter-column';

                    

                    // Quarter Header - Use special style for Backlog

                    const headerClass = quarter === 'Backlog' ? 'backlog-header' : 'quarter-header';

                    quarterColumn.innerHTML = `<div class="${headerClass}">${quarter}</div>`;



                    // 2. Filter projects for this specific System AND Quarter

                    const itemsInQuarter = itemsInSystem.filter(item => item.displayQuarter === quarter);

                    

                    const projectsList = document.createElement('div');

                    projectsList.className = 'space-y-3'; // Adds vertical spacing between cards



                    itemsInQuarter.forEach(item => {

                        const itemDiv = document.createElement('div');

                        itemDiv.className = 'roadmap-item text-sm';

                        itemDiv.setAttribute('data-id', item.id);

                        // Using inline function call to reference the element for expansion

                        itemDiv.setAttribute('onclick', 'toggleDetails(this)'); 



                        // New HTML structure for inline expansion

                        itemDiv.innerHTML = `

                            <p class="font-semibold text-gray-800" title="${item.fullSummary}">${item.summary} <span class="text-xs font-mono text-gray-400">(${item.id})</span></p>



                            <!-- Collapsible Detail Content -->

                            <div class="roadmap-detail hidden mt-2 pt-2 border-t border-gray-100 text-xs text-gray-600 space-y-1">

                                <p class="font-medium text-gray-800">${item.fullSummary}</p>

                                <p><span class="card-label">Team:</span> <span class="card-value text-blue-600">${item.team}</span></p>

                                <p><span class="card-label">Requester:</span> <span class="card-value">${item.requester}</span></p>

                                <p><span class="card-label">Status:</span> <span class="status-pill ${getStatusClass(item.status)}">${item.status}</span></p>

                                <p><span class="card-label">Quarter:</span> <span class="card-value">${item.quarter}</span></p>

                            </div>

                        `;

                        projectsList.appendChild(itemDiv);

                    });



                    quarterColumn.appendChild(projectsList);

                    flowContainer.appendChild(quarterColumn);

                });



                systemLane.appendChild(flowContainer);

                mainContainer.appendChild(systemLane);

            });

        }



        // Removed showModal and closeModal functions

        

        // Close modal when clicking outside (on the overlay) - this listener is now effectively unused but remains harmless

        document.getElementById('item-modal')?.addEventListener('click', (e) => {

            if (e.target.id === 'item-modal') {

                // closeModal is now gone, this block does nothing, but the element itself is removed from HTML

            }

        });

    </script>

</body>

